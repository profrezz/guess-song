extends layout

block content
    section(class="featured")
      div(class="content-wrapper")
          hgroup(class="title")
              h1=title 
              h2 Modify this template to jump-start your node.js express application.
          p
            | To learn more about node.js, visit
            a(href="http://nodejs.org", title="node.js Website", target="_blank") http://nodejs.org/
            |. The page features 
            span(class="highlight") videos, tutorials, and samples
            | to help you get the most from node.js. If you have any questions about node.js and Windows Azure, visit 
            a(href="https://www.windowsazure.com/en-us/develop/nodejs/", title="node.js on Windows Azure", target="_blank") our dev center
            |.

    section(class="main-content")
      h3 Raadom Songs from Youtube:
      ol(class="round")
          li(class="one")
              h5 What's this song name ? guess the wording as fast as possible
              p 
                | You have 40 seconds left! 
                include quize.html
          li(class="two")
              h5 Stopwatch is running! timmer will be decreased every seconds
              p 
                | Now you have {{totalseconds}} seconds
          li(class="three")
              h5 Typing input form to answer the question
              p 
                include typed.html
                //- div(class="type-wrap frame")
                //-   div(id="typed-strings")
                //-     -for(i=0;i<5;i++)
                //-       span
                //-         b {{quizeWord}}
                //-     span(id="typed")
          li(class="four")
              h5 Get Score
                | Once you answer correct question, you got score +120 peer answer and main stopwatch would be on hold until next question appear<br/><br/>
              button(class="btn btn-success") Let's Play
    script(type="text/javascript").
        var t = 10;
        console.log(t);
        $(function(){
            $("#typed").typed({
                // strings: ["Typed.js is a <strong>jQuery</strong> plugin.", "It <em>types</em> out sentences.", "And then deletes them.", "Try it out!"],
                stringsElement: $('#typed-strings'),
                typeSpeed: 30,
                backDelay: 500,
                loop: false,
                contentType: 'html', // or text
                // defaults to false for infinite loop
                loopCount: false,
                //- callback: function(){ foo(); },
                //- resetCallback: function() { newTyped(); }
            });

            $(".reset").click(function(){
                $("#typed").typed('reset');
            });

        });